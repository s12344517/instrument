    <td>\u0412\u0440\u0435\u043c\u044f \u0437\u0430\u0440\u044f\u0434\u043a\u0438<\/td>\n      <td>1 \u0447\u0430\u0441<\/td>\n    <\/tr>\n    <tr>\n      <td>\u041a\u0432\u0430\u0434\u0440\u0430\u0442 \u043f\u043e\u0434 \u0433\u043e\u043b\u043e\u0432\u043a\u0438<\/td>\n      <td>1\/2 \u0434\u044e\u0439\u043c\u0430<\/td>\n    <\/tr>\n    <tr>\n      <td>\u0420\u0435\u0436\u0438\u043c\u044b \u0440\u0430\u0431\u043e\u0442\u044b<\/td>\n      <td>2 \u0441\u043a\u043e\u0440\u043e\u0441\u0442\u0438, \u0440\u0435\u0432\u0435\u0440\u0441<\/td>\n    <\/tr>\n    <tr>\n      <td>\u041f\u043e\u0434\u0441\u0432\u0435\u0442\u043a\u0430<\/td>\n      <td>\u0415\u0441\u0442\u044c, \u043f\u043e\u0434 \u043f\u0430\u0442\u0440\u043e\u043d\u043e\u043c<\/td>\n    <\/tr>\n    <tr>\n      <td>\u041a\u043e\u043c\u043f\u043b\u0435\u043a\u0442\u0430\u0446\u0438\u044f<\/td>\n      <td>\u0413\u043e\u043b\u043e\u0432\u043a\u0438, \u043f\u0435\u0440\u0447\u0430\u0442\u043a\u0438, \u0431\u0435\u0440\u0443\u0448\u0438, \u043a\u0435\u0439\u0441, \u0440\u0435\u043c\u0435\u043d\u044c<\/td>\n    <\/tr>\n    <tr>\n      <td>\u0413\u0430\u0440\u0430\u043d\u0442\u0438\u044f<\/td>\n      <td>5 \u043b\u0435\u0442<\/td>\n    <\/tr>\n  <\/tbody>\n<\/table>\n\n<h4>\u041f\u043b\u044e\u0441\u044b<\/h4>\n<ul>\n  <li>\u041e\u0447\u0435\u043d\u044c \u0432\u044b\u0441\u043e\u043a\u0430\u044f \u043c\u043e\u0449\u043d\u043e\u0441\u0442\u044c \u2014 1800 \u041d\u043c<\/li>\n  <li>\u0411\u044b\u0441\u0442\u0440\u0430\u044f \u0437\u0430\u0440\u044f\u0434\u043a\u0430 \u0438 \u0434\u0432\u0430 \u0430\u043a\u043a\u0443\u043c\u0443\u043b\u044f\u0442\u043e\u0440\u0430<\/li>\n  <li>\u041f\u043e\u043b\u043d\u043e\u0446\u0435\u043d\u043d\u0430\u044f \u043a\u043e\u043c\u043f\u043b\u0435\u043a\u0442\u0430\u0446\u0438\u044f: \u0433\u043e\u043b\u043e\u0432\u043a\u0438, \u043a\u0435\u0439\u0441, \u0440\u0435\u043c\u0435\u043d\u044c, \u043f\u0435\u0440\u0447\u0430\u0442\u043a\u0438<\/li>\n  <li>\u041f\u043e\u0434\u0445\u043e\u0434\u04